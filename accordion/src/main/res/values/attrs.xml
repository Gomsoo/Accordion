<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CollapsibleLayout">
        <attr name="animationDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="CollapsibleHeaderView">
        <attr name="markPosition" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>
        <attr name="title" format="string" />
    </declare-styleable>
</resources>